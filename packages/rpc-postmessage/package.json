{
  "name": "@ceramicnetwork/rpc-postmessage",
  "version": "0.5.0",
  "author": "Ceramic Studio",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicnetwork/js-transports#readme",
  "keywords": [
    "rpc",
    "postmessage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicnetwork/js-transports",
    "directory": "packages/rpc-postmessage"
  },
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "import": "./dist/lib.mjs",
    "require": "./dist/lib.cjs",
    "default": "./dist/lib.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "del dist && tsc --emitDeclarationOnly --skipLibCheck && rollup -c ../../rollup.config.js",
    "lint": "eslint src test --fix",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@ceramicnetwork/rpc-transport": "^0.4.0",
    "@ceramicnetwork/transport-postmessage": "^0.4.0",
    "@ceramicnetwork/transport-subject": "^0.4.0",
    "rpc-utils": "^0.4.0",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "jsdom-worker": "^0.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "jsdom-worker"
    ]
  }
}
